  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#b35900",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["string", "constant.numeric", "constant.language.boolean", "constant.language.null", "constant.character"],
      "settings": {
        "foreground": "#a8a8a8"
      }
    },
    {
      "scope": ["constant", "constant.other"],
      "settings": {
        "foreground": "#ff8000"
      }
    },
    {
      "scope": ["variable", "entity.name.variable", "meta.definition.variable"],
      "settings": {
        "foreground": "#ff8000"
      }
    },
    {
      "scope": ["entity.name.function", "meta.function-call", "support.function"],
      "settings": {
        "foreground": "#ff8000",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["keyword.control", "storage.modifier.control"],
      "settings": {
        "foreground": "#ff8000",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#ff8000",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["keyword", "storage.modifier", "storage.type"],
      "settings": {
        "foreground": "#b35900"
      }
    },
    {
      "scope": ["entity.name.type", "entity.name.class", "support.type", "support.class"],
      "settings": {
        "foreground": "#ff8000"
      }
    },
    {
      "scope": ["meta.preprocessor", "entity.name.function.preprocessor"],
      "settings": {
        "foreground": "#b35900"
      }
    },
    {
      "scope": ["entity.name.tag", "meta.tag"],
      "settings": {
        "foreground": "#ff8000",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["punctuation.definition", "punctuation.separator", "meta.brace"],
      "settings": {
        "foreground": "#b35900"
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#ff8000"
      }
    },
    {
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#ff8000",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#ff8000",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#ff8000",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.underline"],
      "settings": {
        "foreground": "#ff8000",
        "fontStyle": "underline"
      }
    },
    {
      "scope": ["markup.strikethrough"],
      "settings": {
        "foreground": "#ff8000",
        "fontStyle": "strikethrough"
      }
    },
    {
      "scope": ["markup.quote"],
      "settings": {
        "foreground": "#a8a8a8",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.inline.raw", "markup.fenced_code"],
      "settings": {
        "foreground": "#a8a8a8"
      }
    },
    {
      "scope": ["markup.list"],
      "settings": {
        "foreground": "#ff8000"
      }
    },
    {
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#ff8000",
        "fontStyle": "underline"
      }
    },
    {
      "scope": ["invalid"],
      "settings": {
        "foreground": "#f53bff",
        "fontStyle": "bold"
      }
    }
  ]
}
